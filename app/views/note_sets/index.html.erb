<h1>Listing note_sets</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Dateadded</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @note_sets.each do |note_set| %>
      <tr>
        <td><%= note_set.title %></td>
        <td><%= note_set.description %></td>
        <td><%= link_to 'Show', note_set %></td>
        <td><%= link_to 'Edit', edit_note_set_path(note_set) %></td>
        <td><%= link_to 'Destroy', note_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Note set', new_note_set_path %>
